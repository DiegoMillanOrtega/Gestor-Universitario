{
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "parser": "angular",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "bracketSameLine": false,
  "htmlWhitespaceSensitivity": "ignore",
    "singleAttributePerLine": true
      }
    }
  ]
}


